<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>mirror</id>
            <mirrorOf>*,!donghang-releases,!donghang-snapshots</mirrorOf>
            <name>mirror</name>
            <url>https://maven.jdcloud.com/repos/content/groups/public</url>
        </mirror>

    </mirrors>
    <servers>
        <server>
            <id>donghang-releases</id>
            <username>donghang</username>
            <password>donghangpasswd</password>
        </server>
        <server>
            <id>donghang-snapshots</id>
            <username>donghang</username>
            <password>donghangpasswd</password>
        </server>
	    //test
    </servers>
    <profiles>
        <profile>
            <id>donghang</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://maven.jdcloud.com/repos/content/groups/public</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>donghang-releases</id>
					<url>https://116.196.81.34/repos/content/repositories/donghang-releases</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>donghang-snapshots</id>
					<url>https://116.196.81.34/repos/content/repositories/donghang-snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>https://maven.jdcloud.com/repos/content/groups/public</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
                <pluginRepository>
                    <id>donghang-releases</id>
                    <url>https://116.196.81.34/repos/content/repositories/donghang-releases</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>donghang-snapshots</id>
                    <url>https://116.196.81.34/repos/content/repositories/repository-snapshots</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>

    </profiles>

	

    <activeProfiles>
        <activeProfile>donghang</activeProfile>
    </activeProfiles>
</settings>
